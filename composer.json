{
    "name": "kms/froala-editor-bundle",
    "type": "symfony-bundle",
    "description": "Provides a Froala editor integration for Symfony 2.",
    "keywords": [ "froala, editor, wysiwyg" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Simon GUIONNIERE",
            "email": "simon.guionniere@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": "2.*,<2.6",
        "symfony/form": ">=2.1,<2.6-dev",
        "twig/twig": ">=1.1,<2.0-dev"
    },
    "require-dev": {
        "symfony/twig-bundle": "2.*,<2.6",
        "symfony/yaml": "~2.0"
    },
    "autoload": {
        "psr-4": { "KMS\\FroalaEditorBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}